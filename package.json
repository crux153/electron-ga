{
  "name": "@ref/electron-ga",
  "version": "2.0.1",
  "description": "Google Analytics client for Electron applications",
  "engines": {
    "node": ">=14.6",
    "npm": "please-use-yarn",
    "yarn": ">=3.2"
  },
  "packageManager": "yarn@3.2.0",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "build": "rimraf lib && tsc",
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "node-machine-id": "^1.1.12",
    "qs": "^6.10.3"
  },
  "devDependencies": {
    "@electron/remote": "^2.0.8",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.10.3",
    "electron": "^18.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "pinst": "^3.0.0",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@electron/remote": "^2.0.0",
    "electron": ">=14.0.0"
  },
  "files": [
    "lib",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crux153/electron-ga.git"
  },
  "keywords": [
    "google",
    "analytics",
    "electron"
  ],
  "authors": [
    "PÃ©ter Hauszknecht <peter.hauszknecht@jaystack.com> (https://github.com/jayhasyee)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crux153/electron-ga/issues"
  },
  "homepage": "https://github.com/crux153/electron-ga#readme",
  "jest": {
    "globals": {
      "window": true,
      "ts-test": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.spec.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  }
}
